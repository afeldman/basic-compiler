10 REM Test various loop constructs
20 LET SUM = 0
30 FOR I = 1 TO 10
40 LET SUM = SUM + I
50 NEXT I
60 PRINT "Sum 1-10:"
70 PRINT SUM
80 REM Nested loop
90 LET TOTAL = 0
100 FOR X = 1 TO 3
110 FOR Y = 1 TO 3
120 LET TOTAL = TOTAL + X * Y
130 NEXT Y
140 NEXT X
150 PRINT "Nested sum:"
160 PRINT TOTAL
170 END
